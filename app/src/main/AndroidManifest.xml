<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capsule.exal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Standard_A_Activity"
            android:launchMode="standard" />
        <activity
            android:name=".Standard_B_Activity"
            android:launchMode="standard" />
        <activity
            android:name=".SingleTop_A_Activity"
            android:launchMode="singleTop" />
        <activity
            android:name=".SingleTop_B_Activity"
            android:launchMode="singleTop" />
        <activity
            android:name=".SingleTask_A_Activity"
            android:launchMode="singleTask" />
        <activity
            android:name=".SingleTask_B_Activity"
            android:launchMode="singleTask" />
    </application>

</manifest>